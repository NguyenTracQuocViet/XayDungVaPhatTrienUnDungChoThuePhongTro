<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/dark_green"
    tools:context=".ThemDialog.ThemKhachThue">
    <TextView
        android:id="@+id/textView3"
        android:layout_width="414dp"
        android:layout_height="85dp"
        android:background="@drawable/bg_corner_top"
        android:gravity="center"
        android:padding="20dp"
        android:text="Thông Tin Khách Thuê"
        android:textColor="@color/white"
        android:textSize="20dp"
        android:textStyle="bold"
        tools:ignore="MissingConstraints" />


    <TextView
        android:id="@+id/sophongkhachthue"
        android:layout_width="347dp"
        android:layout_height="72dp"
        android:layout_marginTop="104dp"
        android:background="@drawable/_center"
        android:ems="10"
        android:gravity="center"
        android:hint="Số Phòng"
        android:textColorHint="@color/black"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.39"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TouchTargetSizeCheck" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/textInputLayout16"
        android:layout_width="345dp"
        android:layout_height="81dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="200dp"
        android:textColorHint="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <EditText
            android:id="@+id/edttenkhach"
            android:layout_width="match_parent"
            android:layout_height="69dp"
            android:layout_marginTop="290dp"
            android:layout_marginEnd="33dp"
            android:ems="10"
            android:hint="Tên Khách thuê"
            android:inputType="text"
            android:textColorHint="@color/white"
            android:textSize="20dp"
            tools:ignore="TouchTargetSizeCheck" />

    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/textInputLayout18"
        android:layout_width="353dp"
        android:layout_height="107dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="398dp"
        android:textColorHint="@color/white"
        app:counterEnabled="true"
        app:counterMaxLength="10"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.266"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.314"
        app:prefixText="VN +84"
        app:prefixTextColor="@color/red"
        app:startIconDrawable="@drawable/local_phone"
        tools:ignore="MissingConstraints">

        <EditText
            android:id="@+id/edtsdt"
            android:layout_width="341dp"
            android:layout_height="80dp"
            android:digits="0123456789+"
            android:ems="10"
            android:hint="Số Điện Thoại"
            android:inputType="number"
            android:textColorHint="@color/white"
            android:textSize="20dp"
            tools:ignore="TouchTargetSizeCheck" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/textInputLayout17"
        android:layout_width="351dp"
        android:layout_height="107dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="356dp"
        android:textColorHint="@color/white"
        app:counterEnabled="true"
        app:counterMaxLength="12"
        app:layout_constraintBottom_toTopOf="@+id/textInputLayout18"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.034"
        tools:ignore="MissingConstraints">

        <EditText
            android:id="@+id/edtcccd"
            android:layout_width="335dp"
            android:layout_height="76dp"
            android:ems="10"
            android:hint="Số Căn Cước Công Dân"
            android:inputType="number"
            android:textColorHint="@color/white"
            android:textSize="20dp"
            tools:ignore="TouchTargetSizeCheck" />
    </com.google.android.material.textfield.TextInputLayout>

    <Button
        android:id="@+id/btnthem"
        android:layout_width="120dp"
        android:layout_height="58dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="610dp"
        android:layout_marginBottom="64dp"
        android:text="THÊM"
        android:backgroundTint="@color/orange"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnhuy"
        android:layout_width="115dp"
        android:layout_height="53dp"
        android:layout_marginTop="614dp"
        android:layout_marginEnd="56dp"
        android:layout_marginBottom="64dp"
        android:text="HỦY"
        android:backgroundTint="@color/orange"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <RadioGroup
        android:id="@+id/radioGroup"
        android:layout_width="89dp"
        android:layout_height="74dp"
        app:layout_constraintBottom_toTopOf="@+id/textInputLayout17"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.534"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textInputLayout16"
        app:layout_constraintVertical_bias="0.131">

        <RadioButton
            android:id="@+id/GTnam"
            android:layout_width="73dp"
            android:layout_height="34dp"
            android:text="nam"
            tools:ignore="TouchTargetSizeCheck" />

        <RadioButton
            android:id="@+id/GTnu"
            android:layout_width="74dp"
            android:layout_height="wrap_content"
            android:text="nữ"
            tools:ignore="TouchTargetSizeCheck" />
    </RadioGroup>

    <TextView
        android:layout_width="103dp"
        android:layout_height="42dp"
        android:background="@drawable/_center"
        android:gravity="center"
        android:text="Giới Tính"
        android:textSize="20dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textInputLayout17"
        app:layout_constraintEnd_toStartOf="@+id/radioGroup"
        app:layout_constraintHorizontal_bias="0.753"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textInputLayout16"
        app:layout_constraintVertical_bias="0.232" />

</androidx.constraintlayout.widget.ConstraintLayout>