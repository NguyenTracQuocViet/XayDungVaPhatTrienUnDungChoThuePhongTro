<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/dark_green"
    tools:context=".ThemDialog.TaoHopDong">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="414dp"
        android:layout_height="85dp"
        android:layout_marginStart="2dp"
        android:background="@drawable/bg_corner_top"
        android:gravity="center"
        android:text="Tạo Hợp Đồng"
        android:textColor="@color/white"
        android:textSize="25dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/sophonghopdong"
        android:layout_width="117dp"
        android:layout_height="33dp"
        android:layout_marginTop="108dp"
        android:background="@drawable/_center"
        android:ems="10"
        android:gravity="center"
        android:hint="Số Phòng"
        android:textColorHint="@color/black"
        android:textSize="16dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.053"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/tvTenkhachhopdong"
        android:layout_width="258dp"
        android:layout_height="41dp"
        android:layout_marginTop="104dp"
        android:background="@drawable/_center"
        android:ems="10"
        android:gravity="center"
        android:hint="Tên Khách"
        android:textColorHint="@color/black"
        android:textSize="16dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.96"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TouchTargetSizeCheck" />


    <TextView
        android:id="@+id/tvngaybdhopdong"
        android:layout_width="304dp"
        android:layout_height="37dp"
        android:layout_marginTop="180dp"
        android:background="@drawable/_center"
        android:ems="10"
        android:gravity="center"
        android:hint="Ngày Bắt Đầu Thuê"
        android:inputType="text"
        android:textColorHint="@color/black"
        android:textSize="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.626"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView4"
        tools:ignore="TouchTargetSizeCheck" />


    <TextView
        android:id="@+id/tvngaykthopdong"
        android:layout_width="304dp"
        android:layout_height="37dp"
        android:layout_marginTop="248dp"
        android:background="@drawable/_center"
        android:ems="10"
        android:gravity="center"
        android:hint="Ngày Hết Hạn Thuê"
        android:inputType="text"
        android:textColorHint="@color/black"
        android:textSize="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.626"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TouchTargetSizeCheck" />


    <com.google.android.material.textfield.TextInputLayout
        android:layout_width="366dp"
        android:layout_height="68dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="392dp"
        android:textColorHint="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.488"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <EditText
            android:id="@+id/edtSoluongxe"
            android:layout_width="match_parent"
            android:layout_height="58dp"
            android:ems="10"
            android:hint="Số Lượng Xe"
            android:inputType="text"
            android:textColorHint="@color/white"
            android:textSize="16dp"
            tools:ignore="TouchTargetSizeCheck"
            tools:layout_editor_absoluteX="30dp"
            tools:layout_editor_absoluteY="308dp" />

    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:layout_width="372dp"
        android:layout_height="65dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="304dp"
        android:textColorHint="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.363"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <EditText
            android:id="@+id/edtSonguoithue"
            android:layout_width="match_parent"
            android:layout_height="52dp"
            android:ems="10"
            android:hint="Số Người Thuê"
            android:inputType="text"
            android:textColorHint="@color/white"
            android:textSize="16dp"
            tools:ignore="TouchTargetSizeCheck"
            tools:layout_editor_absoluteX="30dp"
            tools:layout_editor_absoluteY="308dp" />

    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:layout_width="376dp"
        android:layout_height="71dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="492dp"
        android:textColorHint="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.628"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="ExtraText,MissingConstraints">>

        <EditText
            android:id="@+id/Tiencochopdong"
            android:layout_width="match_parent"
            android:layout_height="58dp"
            android:ems="10"
            android:hint="Nhập Tiền Cọc(vnđ)"
            android:inputType="text"
            android:textColorHint="@color/white"
            android:textSize="16dp"
            tools:ignore="TouchTargetSizeCheck" />

    </com.google.android.material.textfield.TextInputLayout>

    <Button
        android:id="@+id/btnthemhopdong"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/orange"
        android:text="Thêm"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.139"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.907" />

    <Button
        android:id="@+id/btnhuy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/orange"
        android:text="Hủy"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.845"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.907" />

    <ImageButton
        android:id="@+id/btnLich1"
        android:layout_width="34dp"
        android:layout_height="35dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="180dp"
        android:layout_marginEnd="351dp"
        android:background="@color/dark_green"
        android:contentDescription="@string/appbar_scrolling_view_behavior"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/calendar"
        tools:ignore="TouchTargetSizeCheck" />

    <ImageButton
        android:id="@+id/btnLich2"
        android:layout_width="34dp"
        android:layout_height="35dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="248dp"
        android:layout_marginEnd="351dp"
        android:background="@color/dark_green"
        android:contentDescription="@string/bottom_sheet_behavior"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/calendar"
        tools:ignore="TouchTargetSizeCheck" />


</androidx.constraintlayout.widget.ConstraintLayout>